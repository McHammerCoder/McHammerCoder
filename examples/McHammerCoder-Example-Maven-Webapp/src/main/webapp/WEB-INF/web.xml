<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
      <servlet>
        <servlet-name>ExamplePage01</servlet-name>
        <servlet-class>de.monticore.usage.servlets.ExamplePage01</servlet-class>
        <init-param>
          <param-name>templateFileName</param-name>
          <param-value>xhtmltemplate.xhtml</param-value>
        </init-param>
        <init-param>
          <param-name>url</param-name>
          <param-value>/ex01</param-value>
        </init-param>
    </servlet>
   <servlet>
        <servlet-name>ExamplePage02</servlet-name>
        <servlet-class>de.monticore.usage.servlets.ExamplePage02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExamplePage02</servlet-name>
        <url-pattern>/ex02</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExamplePage01</servlet-name>
        <url-pattern>/ex01</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
