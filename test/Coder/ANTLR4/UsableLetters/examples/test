/** MUHAHA */ 

grammar test{






 fragment token DecimalDigit =
        '0'..'9'
    ;

    fragment token HexDigit =
        (('0'..'9')|('a'..'f')|('A'..'F'))
    ;

   fragment token DecimalIntegerLiteral =
        '0'
        | ('1'..'9') (DecimalDigit)*
    ;

	    Literal =
        StringLiteral
    ;

    NumericLiteral =
        DecimalIntegerLiteral
    ;

}



